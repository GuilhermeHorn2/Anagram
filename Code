#include <iostream>
#include <cmath>
//

//
using namespace std;

int anagrama(string a,string b){
    // return 1 if is an anagram or return 0 if not
    int p = 0;
    if(a.size() == b.size()){
        p++;
    }
    if(p == 0){
        return 0;
    }
    else{
        int k = 0;
        for(int i = 0;i < a.size();i++){
            for(int j = 0;j < a.size();j++){
                if(a[i] == b[j]){
                    k++;
                }
            }
        }
        if(k != a.size()){
            return 0;
        }
        else{
            return 1;
        }
    }
}


